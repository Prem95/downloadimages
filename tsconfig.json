{
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["es6", "dom"],
    "jsx": "react",
    "moduleResolution": "node",
    "types": ["chrome", "react", "react-dom"]
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}